{
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx watchify ./client/main.js -o ./client/bundle.js -v"
    "start": "nodemon ./server/index.js"
  },
  "editor": {
    "formatOnPaste": true,
    "formatOnSave": true
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "supertest": "^6.2.2",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.2"
  }
}
